/*
--------------------------
FONTS
	Richard Fink's Mo' Bulletproofer method: http://readableweb.com/mo-bulletproofer-font-face-css-syntax/
	Museo Sans is A font by Jos Buivenga (exljbris) -> http://www.exljbris.nl used by permission.
--------------------------
*/

/* IE */
@font-face {
	font-family: 'DroidSerif';
	src: url(fonts/DroidSerif-Regular.eot);
}

@font-face {
	font-family: 'MuseoSans';
	src: url(fonts/MuseoSans_500.eot);
}


/* Team Players */
@font-face {
	font-family: 'DroidSerif';
	font-weight: normal;
	font-style: normal;
	font-variant: normal;
	src: url(//:) format("No-IE-404"),
		url(fonts/DroidSerif-Regular.woff) format('woff'),
		url(fonts/DroidSerif-Regular.ttf) format('truetype'),
		url(fonts/DroidSerif-Regular.svg#DroidSerif) format('svg');
}

@font-face {
	font-family: 'MuseoSans';
	font-weight: normal;
	font-style: normal;
	font-variant: normal;
	src: url(//:) format("No-IE-404"),
		url(fonts/MuseoSans_500.woff) format('woff'),
		url(fonts/MuseoSans_500.otf) format('opentype'),
		url(fonts/MuseoSans_500.svg#MuseoSans-500) format('svg');
}



/*
--------------------------
BASIC PAGE STRUCTURE
--------------------------
*/

body {
	margin: 0;
	padding: 0;
  background: #e6e3dc  url(images/branding/bg-page-rev.png) repeat-x left top;
}

#container {
  width: 960px;
  margin: 7px auto 0;
  padding: 0;
  background: #fff;
  position: relative;
}

#header {
  position: relative;
  margin: 0;
  padding: 0;
  width: 960px;
  height: 109px;
  background: url(images/branding/bg-header.png) repeat-y left top;
}

#logo {
  margin: 0 0 0 10px;
/*  padding: 25px 0 0 0;*/
  padding: 18px 0 0;
  float: left;
}

#toolbox {
  margin: 0;
  padding: 10px;
  width: 302px;
  background: url(images/branding/bg-toolbox.png) left bottom;
  float:right;
}

#navigation {
  width: 960px;
/*  height: 50px;*/
  height: 32px;
  background: url(images/branding/bg-navigation.png) no-repeat left top;
}

a:link { color: #2a7490; }
a:visited { color: #666; }
a:hover { color: #222; }
html body div.clearfix, html body span.clearfix {
  background:transparent none repeat scroll 0 0;
  border:0 none;
  clear:both;
  display:block;
  float:none;
  font-size:0;
  height:0;
  margin:0;
  overflow:hidden;
  padding:0;
  visibility:hidden;
  width:0;
}

img { border: 0 none; }

.engine-note {
	margin: 10px;
	color: rgb(127, 127, 127);
	text-align: right;
}

/*
--------------------------
TEXT FORMATTING
--------------------------
*/

body, h1, h2, h3, h4, h5, h6, p, ul, ol, dl, li, dt, dd, blockquote {
/*  font-family: "Lucida Grande", Tahoma, Arial, sans-serif;*/
	font-family: MuseoSans, "Lucida Grande", Tahoma, Arial, sans-serif;
  font-weight: normal;
  color: #333;
}

abbr,
acronym {	border: 0; }

p {
  margin: 10px 0;
/*  font-size: 14px;*/
  font-size: 12px;
  line-height: 18px;
}

#side-column1 p {
/*  font-size: 12px;*/
  font-size: 11px;
  line-height: 16px;
}

h1 {
  font-size: 22px;
  margin: 0 0 15px 0;
  color: #333;
}

h1.title-color-dark {
  color: rgb(51, 51, 51);
  background: transparent url(images/branding/bg-wht-66.png) repeat;
}

h1.title-color-light {
  color: rgb(255, 255, 255);
  background: transparent url(images/branding/bg-dkteal-33.png) repeat;
}

h2 {
  font-size: 18px;
  line-height: 20px;
  margin: 0 0 5px 0;
}

#program-meta h2 {
/*  font-size: 16px;*/
  font-size: 15px;
  line-height: 1.1;
  color: #fff;
}

h3 {
  font-size: 15px;
  margin: 0 0 5px 0;
}

#program-meta h3 {
  font-size: 13px;
  line-height: 1.1;
  color: #fff;
}

h4, h5, h6 {
  font-size: 13px;
  line-height: 16px;
  margin: 0 0 5px 0;
  font-weight: bold;
}

h5, h6 { font-weight: normal; }

h6 { color: rgb(111, 111, 111); }

#side-column1 h3 {
	border: 0;
	color: #8ba542;
	line-height: 1.1;
}

ul {
  margin: 5px 0 10px 0;
  padding: 0;
  list-style: disc;
}

ol {
  margin: 5px 0 10px 0;
  padding: 0;
  list-style: decimal;
}

ul ul,
ol ol { margin-bottom: 0; }

li {
/*  font-size: 14px;*/
  font-size: 12px;
/*  line-height: 20px;*/
  line-height: 18px;
  margin: 0 0 0 25px;
  padding: 5px 0;
}

blockquote {
  margin-left: 40px;
  margin-right: 40px;
}

hr {
	height: 1px;
	border: 0 none;
	color: rgb(230, 227, 220);
	background-color: rgb(230, 227, 220);
}

#side-column1 ul {
  list-style: none;
}

#side-column1 li {
/*  font-size: 12px;*/
  font-size: 11px;
  line-height: 16px;
  margin: 0;
  padding: 5px 0;
}

#program-meta dl {
  margin: 0 0 20px 0;
}

#program-meta dt {
  margin: 10px 0 0 0;
  font-size: 11px;
  line-height: 16px;
  color: #ffc9b6;
}

#program-meta dd {
  margin: 0;
  font-size: 12px;
  line-height: 16px;
  color: #fff;
}

#program-meta dd strong {
  font-size: 14px;
  font-weight: normal;
}

#program-meta a {
  padding-right: 12px;
  background: transparent url(images/branding/bullet-navigation-right.png) right no-repeat;
}

#program-meta a:link,
#program-meta a:visited {
  color: rgb(255, 255, 255);
  text-decoration: none;
}

#program-meta a:hover {
/*  color: rgb(34, 34, 34);*/
  text-decoration: underline;
}

.actionlist { list-style: none; }

.search-results p { margin: 0; }

.search-results .search-info {
	font-size: 9px;
	color: rgb(127, 127, 127);
}

.highlight {
	background-color: yellow;
}
/* vCard/hCard */

.vcard .fn,
.vcard .org,
.vcard .adr,
.vcard .tel,
.vcard .email,
.vcard .street-address,
.vcard .extended-address { display: block; }



